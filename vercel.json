{
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo",
  "cleanUrls": true,
  "framework": null,
  "functions": {
    "api/[[...route]].ts": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}

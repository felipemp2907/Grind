{
  "version": 2,
  "framework": null,
  "functions": {
    "api/**/*.ts": { "runtime": "nodejs20.x" },
    "api/**/*.js": { "runtime": "nodejs20.x" }
  },
  "routes": [
    { "src": "^/trpc(?:/.*)?$", "dest": "/api/index.ts" },
    { "src": "^/health$", "dest": "/api/index.ts" },
    { "src": "^/(.*)$", "dest": "/api/index.ts" }
  ]
}
